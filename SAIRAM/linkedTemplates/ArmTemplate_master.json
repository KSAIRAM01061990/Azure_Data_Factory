{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SAIRAM"
		},
		"AzureBlobStorageforEmployeeFile_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageforEmployeeFile'"
		},
		"AzureSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlServer'"
		},
		"PostgreSql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PostgreSql1'"
		},
		"SQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLServer'"
		},
		"Snowflake_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake'"
		},
		"PostgreSql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"PostgreSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Demo"
		},
		"PostgreSql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"Snowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DEV_LND"
		},
		"Snowflake_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "ACCOUNTADMIN"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SAIRAM_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageforEmployeeFile_connectionString": {
						"value": "[parameters('AzureBlobStorageforEmployeeFile_connectionString')]"
					},
					"AzureSqlServer_connectionString": {
						"value": "[parameters('AzureSqlServer_connectionString')]"
					},
					"Snowflake_password": {
						"value": "[parameters('Snowflake_password')]"
					},
					"PostgreSql1_password": {
						"value": "[parameters('PostgreSql1_password')]"
					},
					"SQLServer_connectionString": {
						"value": "[parameters('SQLServer_connectionString')]"
					},
					"Snowflake_properties_typeProperties_database": {
						"value": "[parameters('Snowflake_properties_typeProperties_database')]"
					},
					"Snowflake_properties_typeProperties_role": {
						"value": "[parameters('Snowflake_properties_typeProperties_role')]"
					},
					"PostgreSql1_properties_typeProperties_server": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_server')]"
					},
					"PostgreSql1_properties_typeProperties_database": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_database')]"
					},
					"PostgreSql1_properties_typeProperties_username": {
						"value": "[parameters('PostgreSql1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "SAIRAM_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'SAIRAM_ArmTemplate_0')]"
			]
		}
	]
}